<div class="centered">
  <h1>Sign In</h1>
  <%= form_tag sessions_create_path, :method => :post do %>
    <table class="center">
        <tr>
        <td><%= label_tag :email %></td>
        <td><%= text_field_tag :email %></td>
      </tr>
      <tr>
        <td><%= label_tag :password %></td>
        <td><%= password_field_tag :password %></td>
      </tr>
    </table>
    <%= submit_tag 'Sign In', :class => 'btn'%>
  <% end %>
</div>
